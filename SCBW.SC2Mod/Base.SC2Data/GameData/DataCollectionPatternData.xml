<?xml version="1.0" encoding="utf-8"?>
<Catalog>
    <CDataCollectionPattern id="UnitBaseSCBWEmpty">
        <Fields Reference="Unit,^ParamId^,LifeStart"/>
        <Fields Reference="Unit,^ParamId^,LifeMax"/>
        <Fields Reference="Unit,^ParamId^,LifeArmor"/>
        <Fields Reference="Unit,^ParamId^,Food"/>
        <Fields Reference="Unit,^ParamId^,Sight"/>
        <Fields Reference="Unit,^ParamId^,Speed"/>
        <Fields Reference="Unit,^ParamId^,Attributes"/>
        <Fields Reference="Unit,^ParamId^,CostResource[Minerals]" NameOverride="DataCollectionPattern/UpgradeMineralCost"/>
        <Fields Reference="Unit,^ParamId^,CostResource[Vespene]" NameOverride="DataCollectionPattern/UpgradeVespeneCost"/>
        <Fields Reference="Unit,^ParamId^,BuildTime"/>
        <Fields Reference="Button,^ParamId^,DefaultButtonLayout"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="SCBW_Weapon">
        <Fields Reference="Effect,^ParamId^@Damage,Amount" NameOverride="DataCollectionPattern/Damage"/>
        <Fields Reference="Effect,^ParamId^@Damage,AttributeBonus[Armored]" NameOverride="DataCollectionPattern/DamageBonusArmored"/>
        <Fields Reference="Effect,^ParamId^@Damage,AttributeBonus[Light]" NameOverride="DataCollectionPattern/DamageBonusLight"/>
        <Fields Reference="Effect,^ParamId^@Damage,AreaArray" NameOverride="DataCollectionPattern/SplashArea"/>
        <Fields Reference="Button,^ParamId^,DefaultButtonLayout"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="SCBW_Upgrade">
        <Fields Reference="Abil,^ParamId^,InfoArray[Research1].Resource[Minerals]" NameOverride="DataCollectionPattern/UpgradeMineralCost"/>
        <Fields Reference="Abil,^ParamId^,InfoArray[Research1].Resource[Vespene]" NameOverride="DataCollectionPattern/UpgradeVespeneCost"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="SCBW_UpgradeLeveled">
        <Fields Reference="Abil,^ParamId^,InfoArray[Research1].Resource[Minerals]" NameOverride="DataCollectionPattern/UpgradeMineralCost@Lvl1"/>
        <Fields Reference="Abil,^ParamId^,InfoArray[Research1].Resource[Vespene]" NameOverride="DataCollectionPattern/UpgradeVespeneCost@Lvl1"/>
        <Fields Reference="Abil,^ParamId^,InfoArray[Research2].Resource[Minerals]" NameOverride="DataCollectionPattern/UpgradeMineralCost@Lvl2"/>
        <Fields Reference="Abil,^ParamId^,InfoArray[Research2].Resource[Vespene]" NameOverride="DataCollectionPattern/UpgradeVespeneCost@Lvl2"/>
        <Fields Reference="Abil,^ParamId^,InfoArray[Research3].Resource[Minerals]" NameOverride="DataCollectionPattern/UpgradeMineralCost@Lvl3"/>
        <Fields Reference="Abil,^ParamId^,InfoArray[Research3].Resource[Vespene]" NameOverride="DataCollectionPattern/UpgradeVespeneCost@Lvl3"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="UnitBaseSCBW" parent="UnitBaseSCBWEmpty"/>
    <CDataCollectionPattern id="UnitBaseSCBWBuilder" parent="UnitBaseSCBW">
        <Fields Reference="Abil,^ParamId^@Build,InfoArray" NameOverride="DataCollectionPattern/BuildFieldName"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="UnitBaseSCBWBuilding" parent="UnitBaseSCBWEmpty">
        <Fields Reference="Unit,^ParamId^,BuildTime" NameOverride="DataCollectionPattern/BuildTimeFieldName"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="UnitBaseSCBWTrainBuilding" parent="UnitBaseSCBWBuilding">
        <Fields Reference="Abil,^ParamId^@Train,InfoArray" NameOverride="DataCollectionPattern/TrainFieldName"/>
    </CDataCollectionPattern>
    <CDataCollectionPattern id="TerranAddonSCBW" parent="UnitBaseSCBWBuilding">
        <Tokens Reference="Unit,^ParamId^,AddedOnUnit"/>
    </CDataCollectionPattern>
</Catalog>
